//
// All profiles begin with this data and overwrite their own
//
Default
	Skill = 40
	Aggression = 50
	ReactionTime = 0.4
	AttackDelay = 0.1
	Teamwork = 40
	AimFocusInitial = 4			// initial focus spread in degrees (from desired center)
	AimFocusDecay = 0.4				// how much focus shrinks per second (.25 = 25% of size after 1 sec)
	AimFocusOffsetScale = 0.04		// controls accuracy when tracking to target (0 == perfect, should always be < 1)
	AimfocusInterval = 0.4			// how often focus is adjusted (smaller intervals means better movement tracking)
	WeaponPreference = none
	Cost = 0
	Difficulty = NORMAL
	VoicePitch = 100
	Skin = 0
	LookAngleMaxAccelNormal = 2000.0
	LookAngleStiffnessNormal = 300.0
	LookAngleDampingNormal = 50.0
	LookAngleMaxAccelAttacking = 5000.0
	LookAngleStiffnessAttacking = 500.0
	LookAngleDampingAttacking = 30.0
End

